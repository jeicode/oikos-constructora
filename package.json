{
  "name": "oikos-constructora",
  "version": "0.0.0",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dl": "ng build && cd dist/ && npx http-server -o -b --cors --proxy http://localhost:8080",
